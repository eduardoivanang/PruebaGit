<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Bienvenido  TITULO!"
        android:layout_centerHorizontal="true"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:id="@+id/text1"
        android:textSize="22dp"/>
    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="HOLA"
        android:id="@+id/boton"
        android:layout_below="@id/text1"
        android:layout_centerHorizontal="true">

    </Button>

    <EditText
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:id="@+id/edit1"
        android:layout_below="@id/boton"
        />


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@id/edit1"
        android:text="AGREGAMOS MAS COSAS"
        android:textSize="25dp"
        android:textAlignment="center"
        android:gravity="center_horizontal"
        android:id="@+id/titulo2"/>


    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/titulo2"
        android:text="https://github.com/eduardoivanang/PruebaGit.githttps://github.com/eduardoivanang/PruebaGit.githttps://github.com/eduardoivanang/PruebaGit.githttps://github.com/eduardoivanang/PruebaGit.githttps://github.com/eduardoivanang/PruebaGit.git"
        android:textSize="16dp"
        android:id="@+id/parrafo1"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/parrafo1"
        android:text="https://github.com/eduardoivanang/PruebaGit.githttps://github.com/eduardoivanang/PruebaGit.githttps://github.com/eduardoivanang/PruebaGit.githttps://github.com/eduardoivanang/PruebaGit.githttps://github.com/eduardoivanang/PruebaGit.git"
        android:textSize="16dp"
        android:id="@+id/parrafo2"
        android:layout_marginTop="15dp"/>

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/parrafo2"
        android:text="https://github.com/eduardoivanang/PruebaGit.githttps://github.com/eduardoivanang/PruebaGit.githttps://github.com/eduardoivanang/PruebaGit.githttps://github.com/eduardoivanang/PruebaGit.githttps://github.com/eduardoivanang/PruebaGit.git"
        android:textSize="16dp"
        android:id="@+id/parrafo3"
        android:layout_marginTop="15dp"
        android:textColor="#546184"/>


</RelativeLayout>